                <div class="card-body">
                  <h5 class="card-title">Operations <span>| Today</span></h5>

                  <table class="table table-borderless datatable">
                    <thead>
                      <tr>
                        <th scope="col">Id_Dr</th>
                        <th scope="col">Surgery Type</th>
                        <th scope="col">Date</th>
                        <th scope="col">Duration</th>
                        <th scope="col">Cost</th>
                      </tr>
                    </thead>
                    <tbody>
                     <% for(let i =0;i<operations.length;i++){ %> 
                      <tr>
                        <th scope="row"><a href="#"><%= i+1 %> </a></th>
                        <td><%= operations[i].Name %> </td>
                        <% let d1 = new Date(operations[i].Time); %> 
                        <td><a href="#" class="text-primary"><%= `${d1.getFullYear()}-${d1.getMonth()}-${d1.getDay()}` %> </a></td>
                        <td><%= operations[i].Duration %> </td>
                        <td>$<%= operations[i].Cost %></td>
                      </tr>
                      <% } %> 
                    </tbody>
                  </table>

                </div>

              </div>
            </div><!-- End Recent Sales -->

            <!-- Top Selling -->
